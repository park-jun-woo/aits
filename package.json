{
  "name": "aits",
  "version": "0.1.0",
  "description": "The first AI-native TypeScript SPA framework, designed for development AI agents.",
  "type": "module",
  "license": "MIT",
  "main": "./dist/aits.js",
  "types": "./dist/aits.d.ts",
  "exports": {
    ".": {
      "import": "./dist/aits.js",
      "types": "./dist/aits.d.ts"
    },
    "./controller": {
      "import": "./dist/Controller.js",
      "types": "./dist/Controller.d.ts"
    },
    "./model": {
      "import": "./dist/Model.js",
      "types": "./dist/Model.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "navigo": "^8.11.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.19"
  },
  "workspaces": [
    "packages/*"
  ]
}
